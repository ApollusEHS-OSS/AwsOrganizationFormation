{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [


        {
            "name": "build.ts",
            "type": "node",
            "request": "launch",
            "args": ["build.ts"],
            "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Current TS Tests File",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/mocha/bin/mocha",
            "args": ["-r", "ts-node/register", "${relativeFile}", "-timeout", "99999999"],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },,
        {
            "name": "all TS Tests File",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/mocha/bin/mocha",
            "args": ["-r", "ts-node/register", "test/**/*.ts", "-timeout", "99999999"],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
        {
            "name": "current ts",
            "type": "node",
            "request": "launch",
            "args": ["${relativeFile}"],
            "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "update",
            "type": "node",
            "request": "launch",
            "args": ["cli.ts", "asdasdasd", "./out1.yml", "--profile", "octest1"],
            "runtimeArgs": ["-r", "ts-node/register"],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "init",
            "type": "node",
            "request": "launch",
            "args": ["cli.ts", "init", "./out.yml", "--profile", "octest1" ],
            "runtimeArgs": ["-r", "ts-node/register"],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "create-change-set",
            "type": "node",
            "request": "launch",
            "args": ["cli.ts", "create-change-set", "./out.yml", "--profile", "olafconijn" ],
            "runtimeArgs": ["-r", "ts-node/register"],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "execute-change-set",
            "type": "node",
            "request": "launch",
            "args": ["cli.ts", "execute-change-set", "56f2e472-159d-41c9-915d-329aae6c987f", "--profile", "olafconijn" ],
            "runtimeArgs": ["-r", "ts-node/register"],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
    ]
}