{
  "name": "aws-organization-formation",
  "version": "0.9.0",
  "description": "Infrastructure as code solution for AWS Organizations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "tsc",
    "lint": "npx eslint './src/**/*.ts'",
    "lint:fix": "npx eslint --fix './src/**/*.ts'",
    "test": "npx jest --config=jest.config.js 'unit-test'",
    "test:ci": "npx jest --ci --collect-coverage --config=jest.config.js 'unit-test'",
    "itests": "npx jest --config=jest.config.js 'integration-test'",
    "start:debug": "npx node --inspect -r ts-node/register -r tsconfig-paths/register cli.ts"
  },
  "files": [
    "dist/*",
    "resources/*"
  ],
  "bin": {
    "org-formation": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OlafConijn/AwsOrganizationFormation.git"
  },
  "author": "Olaf Conijn",
  "license": "MIT",
  "dependencies": {
    "archiver": "^3.1.1",
    "aws-sdk": "^2.519.0",
    "commander": "^2.20.0",
    "ini": "^1.3.5",
    "md5": "^2.2.1",
    "memory-streams": "^0.1.3",
    "uuid": "^3.3.3",
    "yaml-cfn": "^0.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/archiver": "^3.0.0",
    "@types/commander": "^2.12.2",
    "@types/ini": "^1.3.30",
    "@types/jest": "^25.1.0",
    "@types/js-yaml": "^3.12.1",
    "@types/md5": "^2.1.33",
    "@types/node": "^12.7.4",
    "@types/sinon": "^7.0.13",
    "@types/uuid": "^3.4.5",
    "@types/yamljs": "^0.2.30",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "aws-sdk-mock": "^5.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "jest": "^25.1.0",
    "sinon": "^7.4.1",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "AWS",
    "CloudFormation",
    "ControlTower",
    "StackSets",
    "tool"
  ]
}
